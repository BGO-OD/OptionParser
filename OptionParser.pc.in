prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${exec_prefix}/lib64
includedir=${prefix}/include

Name: @CMAKE_PROJECT_NAME@
Description: templated C++ command line option parser with executable to be used in shell scripts
Version: @OPTIONPARSER_LATEST_TAG@
Libs: -L${libdir} -loptions
Libs.private: @PRIVATE_LIBS@
Cflags: -I${includedir}
